{
  "name": "influencer-company-match",
  "version": "0.1.0",
  "description": "Intelligent matching platform for influencers and companies",
  "main": "dist/main/src/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "tsc -p tsconfig.main.json && electron .",
    "start": "concurrently --names \"ML-CHAT,ML-MATCH,BACKEND,FRONTEND\" -c \"bgBlue.bold,bgCyan.bold,bgGreen.bold,bgMagenta.bold\" \"npm run start:ml-chat\" \"npm run start:ml-match\" \"npm run start:backend\" \"npm run start:frontend\"",
    "start:ml-chat": "cd ml-service && .\\venv\\Scripts\\python.exe -m uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "start:ml-match": "cd ml-matching-service && .\\venv\\Scripts\\python.exe -m uvicorn app.main:app --reload --host 0.0.0.0 --port 8001",
    "start:backend": "cd backend && npm run start:dev",
    "start:frontend": "vite",
    "test:ai": "node test-ai-services.js",
    "test:ai:chatbot": "curl http://localhost:8000/health",
    "test:ai:matching": "curl http://localhost:8001/health",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "vite build",
    "build:electron": "tsc -p tsconfig.main.json",
    "electron:build": "electron-builder",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "influencer",
    "matching",
    "collaboration",
    "saas"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.4",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.0",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.2",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@tanstack/react-query": "^5.90.21",
    "@types/stripe": "^8.0.416",
    "axios": "^1.13.5",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.564.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^15.0.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "socket.io-client": "^4.8.3",
    "stripe": "^20.3.1",
    "zustand": "^4.4.7"
  }
}
